<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule match-type="regex">
        <from>^/([0-9a-z]+)$</from>
        <to>/$1.html</to>
    </rule>
    <rule match-type="wildcard">
        <from>/404</from>
        <to>/404.html</to>
    </rule>
</urlrewrite>